{
    "name": "bhasvic-pseudocode",
    "displayName": "BHASVIC Pseudocode",
    "description": "A simple extension for syntax highlighting of BHASVIC's pseudocode standard.",
    "publisher": "David Wheatley (davwheat)",
    "icon": "images/icon.png",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pseudocode",
                "aliases": [
                    "BHASVIC Pseudocode",
                    "pseudocode"
                ],
                "extensions": [
                    ".pseudo"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pseudocode",
                "scopeName": "source.pseudo",
                "path": "./syntaxes/pseudocode.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "pseudocode",
                "path": "./snippets.json"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:pseudocode"
    ],
    "main": "./out/extension.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Willumz/generic-pseudocode-vscode.git"
    },
    "devDependencies": {
        "@types/node": "^15.14.9",
        "@types/vscode": "^1.59.0",
        "typescript": "^4.4.2"
    },
    "dependencies": {}
}
